<h1>Listing project_phases</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Short name</th>
    <th>Sequence</th>
  </tr>

<% @project_phases.each do |project_phase| %>
  <tr>
    <td><%=h project_phase.name %></td>
    <td><%=h project_phase.short_name %></td>
    <td><%=h project_phase.sequence %></td>
    <td><%= link_to 'Show', project_phase %></td>
    <td><%= link_to 'Edit', edit_project_phase_path(project_phase) %></td>
    <td><%= link_to 'Destroy', project_phase, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New project_phase', new_project_phase_path %>