<h1>Editing task_type</h1>

<% form_for(@task_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :complexity_factor %><br />
    <%= f.text_field :complexity_factor %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @task_type %> |
<%= link_to 'Back', task_types_path %>