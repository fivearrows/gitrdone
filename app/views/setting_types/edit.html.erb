<h1>Editing setting_type</h1>

<% form_for(@setting_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @setting_type %> |
<%= link_to 'Back', setting_types_path %>